  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "scripts": {
    "eslint-fix": "eslint --fix lib/",
    "eslint-check": "eslint lib/"
  },
  "dependencies": {
    "md5": "^2.1.0"
  },
  "devDependencies": {
    "babel-eslint": "^6.1.2",
    "eslint": "^3.3.0",
    "eslint-config-atom-build": "^4.0.0"
  }
